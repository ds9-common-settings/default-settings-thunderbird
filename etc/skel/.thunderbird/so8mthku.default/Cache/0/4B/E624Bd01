<addon id="1339">
  <name>Expression Search / GMailUI</name>
  <type id="1">Extension</type>
  <guid>{03EF8A6E-C972-488f-92FA-98ABC2C9F8B9}</guid>
  <slug>gmailui</slug>
  <version>0.8.3</version>
  <status id="4">Fully Reviewed</status>
  <authors>
        <author id="2624362">
      <name>wang opera</name>
